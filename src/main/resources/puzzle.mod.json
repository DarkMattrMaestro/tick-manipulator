{
  "formatVersion": 3,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "display_name": "Tick Manipulator",
  "name": "Tick Manipulator",
  "description": "Manually step ticks with ease.",
  "authors": [
    "DarkMattrMaestro"
  ],
  "meta": {
    "icon": "${mod_id}:icons/icon.png"
  },
  "entrypoints": {
    "init": [
      "com.darkmattrmaestro.tick_manipulator.TickManipulator"
    ],
    "client-init": [
      "com.darkmattrmaestro.tick_manipulator.ClientTickManipulator"
    ]
  },
  "depends": {
    "cosmic-reach": ">=${cr_version}",
    "puzzle-loader-cosmic": ">=${puzzle_loader_cosmic}",
    "puzzle-loader-core": ">=${puzzle_loader_core}"
  },
  "allowedSides": {
    "client": true,
    "server": false
  },
  "mixins": [
    "tick_manipulator.mixins.json",
    {
      "config": "tick_manipulator.client.mixins.json",
      "environment": "client"
    }
  ]
}